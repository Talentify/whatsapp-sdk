{
  "name": "talentify/whatsapp-sdk",
  "description": "Whatsapp SDK",
  "type": "library",
  "homepage": "https://github.com/talentify/whatsapp-sdk",
  "require": {
    "php": ">=7.3.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.0",
    "php-http/guzzle7-adapter": "*",
    "talentify/json-php": "^1.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Talentify/json-php.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Talentify\\Whatsapp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Talentify\\Whatsapp\\": "tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
